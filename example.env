# URL of the Keycloak server, including the realm and protocol
KEYCLOAK_URL=http://your-keycloak-server/realms/your-realm/protocol/openid-connect

# Client ID registered in Keycloak
CLIENT_ID=your-client-id

# Client secret associated with the client ID
CLIENT_SECRET=your-client-secret

# Redirect URI for the application
REDIRECT_URI=http://your-redirect-uri